<launch>
  
  <!-- send pr2 urdf to param server -->
  <include file="$(find lwr_description)/launch/lwr_upload.launch" />

  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="spawn_lwr_model" pkg="gazebo_ros" type="spawn_model" args="$(optenv ROBOT_INITIAL_POSE) -unpause -urdf -param robot_description -model lwr -ros_namespace /gazebo" respawn="false" output="screen" />

  <!-- default bringup script -->
  <include file="$(find lwr_description)/launch/lwr_test.launch" />

</launch>
